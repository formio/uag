{
  "name": "@formio/uag",
  "version": "1.3.1",
  "description": "The Form.io Universal Agent Gateway (UAG).",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build:ts": "tsc",
    "build:copy-templates": "mkdir -p lib/templates && cp src/templates/*.md lib/templates/",
    "build": "npm run clean && npm run build:ts && npm run build:copy-templates",
    "build:docker": "npm run build && docker build -t formio/uag:rc --platform=linux/amd64 .",
    "test": "TS_NODE_PROJECT=tsconfig.test.json mocha --no-node-snapshot",
    "dev": "tsx --no-node-snapshot index.js",
    "start": "node --no-node-snapshot index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@formio/appserver": "^2.3.1",
    "@formio/core": "2.5.1-dev.291.6557e4e",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "chai": "^6.0.1",
    "mocha": "^11.7.4",
    "supertest": "^7.1.4",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.3"
  }
}
